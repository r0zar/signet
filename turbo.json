{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "globalDependencies": [".env*"],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NODE_ENV",
        "VERCEL_URL"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "build:firefox": {
      "dependsOn": [
        "^build:firefox"
      ],
      "env": [
        "NODE_ENV"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": []
    },
    "debug": {
      "cache": false,
      "persistent": true
    },
    "debug:firefox": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:firefox": {
      "cache": false,
      "persistent": true
    },
    "package": {
      "dependsOn": [
        "^package"
      ],
      "env": [
        "NODE_ENV"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "package:firefox": {
      "dependsOn": [
        "^package:firefox"
      ],
      "env": [
        "NODE_ENV"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    }
  }
}
