{
  "name": "clerk-chrome-extension-demo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:firefox": "turbo build:firefox",
    "build:webapp": "turbo build --filter=web-app",
    "debug": "turbo debug",
    "debug:firefox": "turbo debug:firefox",
    "dev": "turbo dev",
    "dev:firefox": "turbo dev:firefox",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "package": "turbo package",
    "package:firefox": "turbo package:firefox"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.3.1",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "overrides": {
      "braces": "^3.0.3",
      "msgpackr": "^1.10.1"
    }
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}